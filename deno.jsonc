{
  "tasks": {
    "start": "deno run src/main.ts",
    "install": "deno install --allow-read --allow-env --allow-run --unstable -rfn n mod.ts"
  },
  "fmt": {
    "options": {
      "semiColons": false
    }
  },
  "lint": {
    "rules": {
      "exclude": ["no-explicit-any"]
    }
  },
  "compilerOptions": {
    "types": ["npm:@types/node"]
  }
}
